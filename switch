#include<stdio.h>
int main()
{
	char grade='';
	switch(grade){
	case 'a':
			printf("excellent\n");
			break;
				case 'b':
				case 'c':
			printf("well done\n");
			break;
				case 'd':
			printf("you passed\n");
			break;
				case 'e':
			printf("better try again\n");
			break;
			defoult:
				printf("invalid grade\n");
			}
			printf("your grade is %c\n",grade);
			return 0;
}
